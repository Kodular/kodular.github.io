---
import Layout from '../layouts/Layout.astro';
---

<Layout title="Pricing | Kodular">
  <!-- Pricing Header Section with clean design -->
  <section class="relative py-16 bg-white overflow-hidden">
    <!-- Background elements -->
    <div class="absolute inset-0 z-0 pointer-events-none overflow-hidden">
      <!-- Subtle grid pattern -->
      <div class="absolute inset-0 bg-[linear-gradient(rgba(0,0,0,0.03)_1px,transparent_1px),linear-gradient(to_right,rgba(0,0,0,0.03)_1px,transparent_1px)] bg-[size:40px_40px]"></div>
      
      <!-- Top-right corner decoration -->
      <div class="absolute -top-20 -right-20 w-96 h-96">
        <div class="absolute top-0 right-0 w-full h-full bg-primary-100 rounded-full opacity-20"></div>
        <div class="absolute top-10 right-10 w-3/4 h-3/4 bg-primary-200 rounded-full opacity-20"></div>
      </div>
      
      <!-- Bottom-left decoration -->
      <div class="absolute bottom-0 left-0 w-64 h-64 opacity-10">
        <div class="absolute inset-0 bg-[linear-gradient(rgba(0,0,0,0.1)_1px,transparent_1px),linear-gradient(to_right,rgba(0,0,0,0.1)_1px,transparent_1px)] bg-[size:16px_16px] transform rotate-12"></div>
      </div>
      
      <!-- Floating circle decoration -->
      <div class="absolute top-1/3 right-1/4 w-20 h-20">
        <div class="absolute inset-0 rounded-full bg-primary-300 opacity-10"></div>
      </div>
    </div>

    <div class="container mx-auto px-4 relative z-10">
      <!-- Section heading with modern typography -->
      <div class="text-center mb-12">
        <h1 class="text-4xl md:text-5xl font-bold text-primary-500 mb-4">Pricing Plans</h1>
        <p class="text-xl text-gray-600 max-w-3xl mx-auto">Choose the perfect plan for your app development needs</p>
        <div class="flex items-center justify-center mt-6 gap-2">
          <span class="inline-flex items-center gap-1 text-sm bg-primary-50 text-primary-600 px-3 py-1 rounded-full border border-primary-100">
            <i class="fa fa-check w-4 h-4"></i>
            <span>No credit card required</span>
          </span>
          <span class="inline-flex items-center gap-1 text-sm bg-primary-50 text-primary-600 px-3 py-1 rounded-full border border-primary-100">
            <i class="fa fa-check w-4 h-4"></i>
            <span>Cancel anytime</span>
          </span>
        </div>
      </div>

      <!-- Premium Card -->
      <div class="max-w-4xl mx-auto bg-white rounded-2xl shadow-xl overflow-hidden mb-16 border border-gray-100">
        <div class="bg-gradient-to-r from-primary-600 to-primary-400 p-8 relative">
          <!-- Decorative elements -->
          <div class="absolute top-0 right-0 w-64 h-64 bg-white/10 rounded-full -mt-12 -mr-12 blur-2xl"></div>
          <div class="absolute bottom-0 left-0 w-48 h-48 bg-white/10 rounded-full -mb-8 -ml-8 blur-xl"></div>
          
          <div class="relative z-10">
            <div class="flex flex-col md:flex-row md:items-center md:justify-between gap-6">
              <div>
                <h2 class="text-3xl font-bold text-white mb-2">Get more from Kodular</h2>
                <p class="text-white/90 text-lg">Upgrade to Premium today and unlock all features</p>
              </div>
              <div class="flex flex-col sm:flex-row gap-4">
                <a href="https://my.kodular.io/premium/#monthly" class="px-6 py-3 bg-white text-primary-600 rounded-lg font-medium hover:bg-gray-100 transition-all duration-300 shadow-md text-center">
                  $3.50/month
                </a>
                <a href="https://my.kodular.io/premium/#yearly" class="px-6 py-3 bg-primary-700 text-white rounded-lg font-medium hover:bg-primary-800 transition-all duration-300 border border-white/20 text-center flex items-center justify-center gap-2">
                  <span>$35/year</span>
                  <span class="bg-primary-500 text-white text-xs px-2 py-0.5 rounded-full">2 months free</span>
                </a>
              </div>
            </div>
          </div>
        </div>

        <!-- Benefits Section -->
        <div class="p-8">
          <h3 class="text-2xl font-bold text-gray-800 mb-6 text-center">What you get with Premium</h3>
          <p class="text-gray-600 mb-8 text-center">Gain unrestricted access to all of Kodular. Build powerful apps without limits.</p>
				
          <!-- Modern comparison table -->  
          <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
            <!-- Feature column -->  
            <div class="bg-gray-50 rounded-xl p-6">
              <h4 class="text-lg font-semibold text-gray-800 mb-12 text-center">Features</h4>
              <ul class="space-y-6">
                <li class="font-medium text-gray-700 min-h-[60px] flex items-center">Number of projects</li>
                <li class="font-medium text-gray-700 min-h-[60px] flex items-center">Total asset size</li>
                <li class="font-medium text-gray-700 min-h-[60px] flex items-center">Number of extensions per project</li>
                <li class="font-medium text-gray-700 min-h-[60px] flex items-center">Monetization extensions</li>
                <li class="font-medium text-gray-700 min-h-[60px] flex items-center">Monetization components</li>
                <li class="font-medium text-gray-700 min-h-[60px] flex items-center">Monetization approvals</li>
              </ul>
            </div>
            
            <!-- Free plan column -->  
            <div class="bg-white rounded-xl p-6 border border-gray-200 shadow-sm">
              <div class="text-center mb-6">
                <h4 class="text-lg font-semibold text-gray-800">Free</h4>
                <p class="text-sm text-gray-500 mt-1">Get started with basic features</p>
              </div>
              <ul class="space-y-6">
                <li class="text-gray-600 min-h-[60px] flex items-center justify-center">
                  <span class="bg-gray-100 rounded-full px-3 py-1 text-sm">10</span>
                </li>
                <li class="text-gray-600 min-h-[60px] flex items-center justify-center">
                  <span class="bg-gray-100 rounded-full px-3 py-1 text-sm">5MB</span>
                </li>
                <li class="text-gray-600 min-h-[60px] flex items-center justify-center">
                  <span class="bg-gray-100 rounded-full px-3 py-1 text-sm">5</span>
                </li>
                <li class="text-gray-600 text-sm min-h-[60px] flex items-center justify-center text-center">Cannot be used in projects</li>
                <li class="text-gray-600 text-sm min-h-[60px] flex items-center justify-center text-center">Only Google Ad Manager components, with commission</li>
                <li class="text-gray-600 text-sm min-h-[60px] flex items-center justify-center text-center">Regular review process</li>
              </ul>
            </div>
            
            <!-- Premium plan column -->  
            <div class="bg-white rounded-xl p-6 border border-primary-200 shadow-md relative">
              <!-- Recommended badge -->  
              <div class="absolute -top-3 left-1/2 -translate-x-1/2 bg-primary-500 text-white text-xs px-3 py-1 rounded-full font-medium">RECOMMENDED</div>
              
              <div class="text-center mb-6">
                <h4 class="text-lg font-semibold text-primary-600">Premium</h4>
                <p class="text-sm text-gray-500 mt-1">Unlock all features</p>
              </div>
              <ul class="space-y-6">
                <li class="text-gray-700 min-h-[60px] flex items-center justify-center">
                  <span class="bg-primary-50 text-primary-600 rounded-full px-3 py-1 text-sm font-medium">Unlimited</span>
                </li>
                <li class="text-gray-700 min-h-[60px] flex items-center justify-center">
